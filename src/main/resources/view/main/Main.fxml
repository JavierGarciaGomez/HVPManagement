<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane maxHeight="-Infinity"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            stylesheets="@/style/mainStyle.css" fx:controller="com.JGG.WeeklyScheduler.controller.main.MainController" fx:id="rootPane">
    <HBox prefHeight="100.0" spacing="5">
        <VBox spacing="5">
            <Button mnemonicParsing="false" text="Login" maxWidth="Infinity" onAction="#login"/>
            <Button mnemonicParsing="false" text="Attendance control" maxWidth="Infinity"/>
            <Button mnemonicParsing="false" text="Payroll" maxWidth="Infinity"/>
            <Button mnemonicParsing="false" text="Comissions" maxWidth="Infinity"/>
        </VBox>
        <VBox spacing="5">
            <Button mnemonicParsing="false" text="Manage users"  maxWidth="Infinity"/>
            <Button mnemonicParsing="false" text="Schedule" maxWidth="Infinity"/>
            <Button mnemonicParsing="false" text="Services" maxWidth="Infinity"/>
            <Button mnemonicParsing="false" text="Desk Account" maxWidth="Infinity"/>
        </VBox>
    </HBox>
</AnchorPane>
